module seven_seg (
    input char[5],
    output segs[7]
  ) {

//11 10 7 5 4 2 1
  always {
    case (char) {
      0: segs = 7b0001000;
      1: segs = 7b1101011;
      2: segs = 7b0100100;
      3: segs = 7b0100001;
      4: segs = 7b1000011;
      5: segs = 7b0010001;
      6: segs = 7b0010000;
      7: segs = 7b0101011;
      8: segs = 7b0000000;
      9: segs = 7b0000011;
      10: segs = 7b0000010; //A
      11: segs = 7b1010000; //b
      12: segs = 7b0011100; //C
      13: segs = 7b1100000; //d
      14: segs = 7b0010100; //E
      15: segs = 7b0010110; //F
      16: segs = 7b1110000; //o
      17: segs = 7b1110010; //n
      18: segs = 7b1110110; //r
      19: segs = 7b1000010; //X
      20: segs = 7b1010100; //t
      21: segs = 7b1000001; //Y
      default: segs = 7b1111111;
    }
  }
}